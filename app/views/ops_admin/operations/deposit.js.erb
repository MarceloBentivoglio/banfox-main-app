document.querySelector('.table_wo_bundle').insertAdjacentHTML('beforeend', '<%= j render partial: "shared/tables/table_wo_bundle/row", collection: @installments, as: :item %>');
<% if @installments.next_page %>
  document.querySelector('.pagination').innerHTML = '<%= j will_paginate @installments %>';
